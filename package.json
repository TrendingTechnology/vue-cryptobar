{
  "name": "vue-cryptobar",
  "version": "1.5.5",
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name onur ./src/components/cryptobar.vue",
    "e2e": "vue-cli-service e2e",
    "e2e:open": "vue-cli-service e2e:open",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ajv": "^6.6.1",
    "ajv-keywords": "^3.2.0",
    "cryptocompare": "^0.7.0",
    "vue": "^2.5.17"
  },
  "main": "./dist/onur.common.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/onurrozkaan/vue-cryptobar.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "node": "^11.2.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  },
  "author": "Onur OZKAN",
  "keywords": [
    "cryptobar",
    "cryptocurrency",
    "bitcoin",
    "ethereum",
    "api",
    "price"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
